#!/usr/bin/env groovy


node('windows'){
    
    stage('checkout')
    {

         checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/devopsly12/exampleapp.git']])

    }
    stage('Build')

    {
      bat "dotnet build C:/jenkins/workspace/dotnet_cicd/examplewebapp/bin/Debug/net6.0/examplewebapp.dll"
    }
    
}
