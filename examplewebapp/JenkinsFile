node('windows'){
    
    stage('checkout')
    {

         checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/devopsly12/exampleapp.git']])

    }
    stage('Build')

    {
      def project_path = 'C:\jenkins\workspace\dotnet_cicd\examplewebapp\bin\Debug\net6.0'
      bat  'dotnet build ${project_path}'
    }
    
}
